{
  "name": "react-sweet",
  "version": "0.0.1",
  "description": "a tool and function lib of react hook",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "build:publish": "sh /bin/publish.sh",
    "debug": "gulp compiler",
    "eslint": "eslint --ext .tsx,.ts --fix ./src ./cli"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run eslint",
      "prettier .prettierrc.js --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenzhaolong/react-sweet.git"
  },
  "keywords": [
    "react-sweet"
  ],
  "author": "dcv",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chenzhaolong/react-sweet/issues"
  },
  "homepage": "https://github.com/chenzhaolong/react-sweet#readme",
  "devDependencies": {
    "gulp-watch": "^5.0.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "^5.0.1",
    "prettier": "^1.18.2"
  }
}
